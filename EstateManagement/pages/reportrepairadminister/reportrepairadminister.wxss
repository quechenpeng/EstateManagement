page {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #e7e7e7;
}

.navbar {
  flex: none;
  display: flex;
  background: #fff;
}

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
  font-size: 13px;
}

.navbar .item.active {
  color: #5a79b0;
}

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #5a79b0;
}

.tips {
