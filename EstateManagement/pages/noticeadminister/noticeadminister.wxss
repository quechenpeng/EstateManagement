page {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #e7e7e7;
}

.tips {
  width: 100%;
  height: 30px;
  line-height: 30px;
  font-size: 13px;
  text-align: center;
  color: rgb(98, 99, 100);
}

.form {
  margin-bottom: 70px;
}

.modular {
  width: 100%;
  height: 70px;
  margin-bottom: 8px;
  background-color: #fff;
  display: flex;
}

navigator {
  width: 100%;
  height: 70px;
  background-color: #fff;
  display: flex;
}

.left {
  width: 75%;
  height: 100%;
}

.right {
  width: 25%;
  height: 100%;
}

.lefttop {
  width: 100%;
  height: 50%;
}

.leftbottom {
  width: 100%;
  height: 50%;
}

.time {
  width: 100%;
  height: 100%;
  line-height: 35px;
  margin-left: 10px;
  font-size: 13px;
  font-weight: 450;
}

.title {
  width: 100%;
  height: 100%;
  line-height: 35px;
  margin-left: 10px;
  font-size: 12px;
  font-weight: 400;
}

.readingvolume {
  font-size: 10px;
  color: #808b9f;
  margin-top: 15px;
}

.bottom {
  width: 100%;
  height: 70px;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}

.circle {
  width: 50px;
  height: 50px;
  line-height: 50px;
  border-radius: 50px;
  background-color: #41a5ee;
  border: 1px solid #41a5ee;
  margin: 0 auto;
  text-align: center;
  margin-top: 10px;
}

.create{
  color: #fff;
}